unit model.PedidoCompleto;

interface

uses
  System.Generics.Collections, model.TPedidoProdutos,
  model.TProduto, model.TPedidoDadosGerais, model.TCliente,

type
  TPedidoCompleto = class
  public
    Pedido: TPedidoDadosGerais;
    Cliente: TCliente;
    Produto: TProduto;
    PedidoProdutos: TObjectList<TPedidoProdutos>;

    constructor Create;
    destructor Destroy; override;
  end;

implementation

constructor TPedidoCompletoDTO.Create;
begin
  Pedido := TPedidosDadosGeraisDTO.Create;
  Cliente := TClientesDTO.Create;
  Produto := TProdutosDTO.Create;
  PedidoProdutos := TObjectList<TPedidosProdutosDTO>.Create;
end;

destructor TPedidoCompletoDTO.Destroy;
begin
  Pedido.Free;
  Cliente.Free;
  Produto.Free;
  PedidoProdutos.Free;
  inherited;
end;

end.

